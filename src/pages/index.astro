---
// Component Imports
import BaseHead from '../components/base/BaseHead.astro'
import LogoLine from '../components/base/LogoLine.astro'
import Main from '../components/Main'
const someProps = {
	count: 0
}
// Full Astro Component Syntax:
// https://docs.astro.build/core-concepts/astro-components/
---
<html lang="en">
	<head>
		<BaseHead description='cv' title='resume'></BaseHead>
	</head>
	<body>
		<LogoLine></LogoLine>
		<main>
			<Main {...someProps} client:visible>
				<h1>Hello, everyone!</h1>
			</Main>
		</main>
	</body>
</html>
